{
  "name": "quickvet-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0"
  },
  "scripts": {
    "dev": "vite --config vite.config.js",
    "checkout": "set PORT=3001 && react-scripts start",
    "dashboard": "vite --config vite.dashboard.config.js",
    "build": "vite build --config vite.config.js",
    "build:checkout": "react-scripts build",
    "build:dashboard": "vite build --config vite.dashboard.config.js"
  },
  "proxy": "http://localhost:8000",
  "browserslist": {
    "production": [">0.2%", "not dead"],
    "development": ["last 1 chrome version"]
  }
}
